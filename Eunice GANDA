		static void Main(string [] args)
		{
			//main method
			Console.WriteLine("--------------------------------------------------------------------------------");
		    Console.WriteLine("    *****  *****  *    * *     * ***** ****  ******* *****  *****  *    *      ");
		    Console.WriteLine("   *      *     * * *  *  *   *  ***** *   *    *      *   *     * * *  *      ");
		    Console.WriteLine("   *      *     * *  * *   * *   *     * *      *      *   *     * *  * *      ");
		    Console.WriteLine("    *****  *****  *    *    *    ***** *    *   *    *****  *****  *    *      ");
		    Console.WriteLine("--------------------------------------------------------------------------------");
			Console.WriteLine("                           Select A if your an Admin.");
			Console.WriteLine("                           Select U if your an Admin.");Line();Space();
			Console.Write("Enter your choice: "); 
			string choice = Console.ReadLine();
			
		    switch(choice)
			{
				case "A":  //If the user choose A, the code will ask for a password which can only be open by the Admin. 
				    PassCode(); //passcode can be seen in the passcode method.
					break;
                case "U": //If the user choose U, the code will continue the code to the main menu.
                    MainMenu();
                    UserActivity();					
				    Looping();
					break;
				default:
			        Console.WriteLine("Enter Capital LETTER only!! Please try again.");
					Main(null);
			        break;
			}
		}
		static void PassCode() //Asking the Admin for passcode.
		{
			Console.Write("Please Enter your Passcode: "); 
			string passcode = Console.ReadLine();
             
            if (passcode == "ganda"){  // PASSCODE IS "ganda" only!!
				ExitIdol();} //If the admin puut the right passcode the code will exit.
			else {
				Console.WriteLine("Incorrect Passcode! please re-enter your passcode."); //If the admin input the wrong passcode the code will continue to ask for passcode until the right passcode was inputted. 
				PassCode();} 
		}
		static void MainMenu() //Display Options
		{
			Line();Space();
		    Console.WriteLine("                           SELECT PREFERRED ACTIVITY");
