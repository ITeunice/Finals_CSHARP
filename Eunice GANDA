		static void LocalConvertion() //PHP Convert to Any Currency available
		{
			AvailableRates();
			Space();
			Console.Write("Enter PHP PESO amount you want to convert:  "); 
			double num1 = Convert.ToDouble(Console.ReadLine());
			
			Space();
			Console.WriteLine("Input currency: ex.USD");Space();
			Console.Write("To: ");
			string cur3 = Console.ReadLine();  
			Line();Space();
			
                                    if (cur3 == "AUD" || cur3 == "aud"){ 
									     double n1 = num1 * 0.0027;
									         Console.WriteLine("You converted "+ num1+ " PHP to " + Math.Round(n1,2) + " AUD");}
                    	            else if (cur3 == "BHD" || cur3 == "bhd"){ 
									     double n1 = num1 * 0.0069;
									         Console.WriteLine("You converted "+ num1+ " PHP to " + Math.Round(n1,2) + " BHD");}
			                        else  if (cur3 == "GBP" || cur3 == "gbp"){
									     double n1 = num1 * 0.015;
									         Console.WriteLine("You converted "+ num1+ " PHP to " + Math.Round(n1,2) + " GBP");}
						            else  if (cur3== "CAD" || cur3 == "cad"){
									     double n1 = num1 * 0.025;
									         Console.WriteLine("You converted "+ num1+ " PHP to " + Math.Round(n1,2) + " CAD");}
						            else  if (cur3 == "CNY" || cur3 == "cny"){
									     double n1 = num1 * 0.13;
									         Console.WriteLine("You converted "+ num1+ " PHP to " + Math.Round(n1,2) + " CNY");}
						            else  if (cur3 == "DKK" || cur3 == "dkk"){ 
										 double n1 = num1 * 0.13;
									         Console.WriteLine("You converted "+ num1+ " PHP to " + Math.Round(n1,2) + " DKK");}
						            else  if (cur3 == "AED" || cur3 == "aed"){
										 double n1 = num1 * 0.067;
									         Console.WriteLine("You converted "+ num1+ " PHP to " + Math.Round(n1,2) + " AED");}
						            else  if (cur3 == "EUR" || cur3 == "eur"){
										 double n1 = num1 * 0.017;
									         Console.WriteLine("You converted "+ num1+ " PHP to " + Math.Round(n1,2) + " EUR");}
						            else  if (cur3 == "HKD" || cur3 == "hkd"){
										 double n1 = num1 * 0.14;
									         Console.WriteLine("You converted "+ num1+ " PHP to " + Math.Round(n1,2) + " HKD");}
						            else  if (cur3 == "INR" || cur3 == "inr"){
										 double n1 = num1 * 1.51;
									         Console.WriteLine("You converted "+ num1+ " PHP to " + Math.Round(n1,2) + " INR");}
						            else  if (cur3 == "JPY" || cur3 == "jpy"){ 
									     double n1 = num1 * 2.49;
									         Console.WriteLine("You converted "+ num1+ " PHP to " + Math.Round(n1,2)  + " JPY");}
						            else if (cur3 == "KWD" || cur3 == "kwd"){
										 double n1 = num1 * 0.0056;
									         Console.WriteLine("You converted "+ num1+ " PHP to " + Math.Round(n1,2) + " BHD");}
			                        else  if (cur3 == "MYR" || cur3 == "myr"){
										 double n1 = num1 * 0.081;
									         Console.WriteLine("You converted "+ num1+ " PHP to " + Math.Round(n1,2) + " MYR");}
						            else  if (cur3 == "MXN" || cur3 == "mxn"){
										 double n1 = num1 * 0.33;
									         Console.WriteLine("You converted "+ num1+ " PHP to " + Math.Round(n1,2) + " MXN");}
						            else  if (cur3 == "QAR" || cur3 == "qar"){
										 double n1 = num1 * 0.066;
									         Console.WriteLine("You converted "+ num1+ " PHP to " + Math.Round(n1,2) + " QAR");}
						            else  if (cur3 == "SAR" || cur3 == "sar"){
										 double n1 = num1 * 0.068;
									         Console.WriteLine("You converted "+ num1+ " PHP to " + Math.Round(n1,2) + " SAR");}
						            else  if (cur3 == "SGD" || cur3 == "sgd"){
										 double n1 = num1 * 0.025;
									         Console.WriteLine("You converted "+ num1+ " PHP to " + Math.Round(n1,2) + 	" SGD");}
						            else  if (cur3 == "KRW" || cur3 == "krw"){
										 double n1 = num1 * 23.94;
									         Console.WriteLine("You converted "+ num1+ " PHP to " + Math.Round(n1,2) + " KRW");}
						            else  if (cur3 == "SEK" || cur3 == "sek"){
										 double n1 = num1 * 0.19;
									         Console.WriteLine("You converted "+ num1+ " PHP to " + Math.Round(n1,2) + " SEK");}
						            else  if (cur3 == "CHF" || cur3 == "chf"){
										 double n1 = num1 * 0.017;
									         Console.WriteLine("You converted "+ num1+ " PHP to " + Math.Round(n1,2) + " CHF");}
						            else  if (cur3 == "THB" || cur3 == "thb"){
										 double n1 = num1 * 0.64;
									         Console.WriteLine("You converted "+ num1+ " PHP to " + Math.Round(n1,2) + " THB");}
						            else  if (cur3 == "USD" || cur3 == "usd"){
										 double n1 = num1 * 0.018;
									         Console.WriteLine("You converted "+ num1+ " PHP to " + Math.Round(n1,2) + " USD");}
									else{
							                Console.WriteLine("I'm sorry! I can't find that currency please enter available currency.");
							                ConvertionRates();}
		}
		
	    static void AvailableRates()
		{
			Space();
			Console.WriteLine("_____________________________AVAILABLE CURRENCIES________________________________"); Space();
			Console.WriteLine("  AUD - Australian Dollar                         KWD - Kuwaiti Dinar  ");Space();
			Console.WriteLine("  BHD - Bahraini Dinar                            MYR - Malaysian Ringgit");Space();
		    Console.WriteLine("  GBP - British Pound                             MXN - Mexican Peso  ");Space();
			Console.WriteLine("  CAD - Canadian Dollar                           QAR - Qatari Riyal");Space();
			Console.WriteLine("  CNY - Chinese Yuan Renminbi                     SAR - Saudi Arabia RiyaL");Space();
			Console.WriteLine("  DKK - Danish Krone                              SGD - Singapore Dollar");Space();
			Console.WriteLine("  AED - Emirati Dirham                            KRW - South Korean Won");Space();
			Console.WriteLine("  EUR - Euro                                      SEK - Swedish Kron");Space();
			Console.WriteLine("  HKD - Hong Kong Dollar                          CHF - Swiss Franc");Space();
			Console.WriteLine("  INR - Indian Rupee                              THB - Thai Baht");Space();
			Console.WriteLine("  JPY - Japanese Yen                              USD - US Dollar");
			Line();
		}
		
		static void Looping()
		{
			Line();Space();   
			Console.WriteLine("Do you want to continue using this application? ");
			Console.WriteLine("Enter any key to continue.");
			Console.WriteLine("Enter no to exit.");
			
			string response = Console.ReadLine();
				if (response == "no" || response == "NO" || response == "nO" || response == "No"){
                ExitIdol();}
				else{
				Main(null);}	
		}			
		static void Space()
		{
			Console.WriteLine("  ");
		}
		
		static void Line()
		{
			Console.WriteLine("_________________________________________________________________________________");
		}
		
	    static void ExitIdol()
		{
		    System.Environment.Exit(1);
		}
	}		
}
