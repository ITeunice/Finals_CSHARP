		static void UserActivity() //Get User Option
		{
            Console.Write("Input your choice: ");
		    string choice = Console.ReadLine();
			
			if (choice == "view" || choice == "VIEW"){ 
		        ViewRates();}
			else if (choice == "intern" || choice == "INTERN"){
				ConvertionRates();}	
			else if (choice == "local" || choice == "LOCAL"){
		     	LocalConvertion();}
			else{
			    Console.WriteLine("I can't find your input. Please choose in the available option.");
			    UserActivity();}
		}	

		
